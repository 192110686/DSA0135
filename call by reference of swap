#include<iostream>
using namespace std;
void swap(int num1,int num2)
{
	int temp;
	temp=num1;
	num1=num2;
	num2=temp;
}
int main()
{
	int num1,num2;
	cout<<"enter the value of num1:";
	cin>>num1;
	cout<<"enter the value of num2:";
	cin>>num2;
 if(num1 && num2 >0)
	{
	cout<<"\n before sswapping"<<"num1="<<num1<<"num2="<<num2;
	swap(num1,num2);
	cout<<"after the swappping"<<"num1="<<num2<<"num2="<<num1;
   }
 else if(num1 && num2 <0)
   {
   	cout<<"\n before sswapping"<<"num1="<<num1<<"num2="<<num2;
	swap(num1,num2);
	cout<<"after the swappping"<<"num1="<<num2<<"num2="<<num1;
   }
 else
   {
   	cout<<"invalid input";
   }
   return 0;
}
